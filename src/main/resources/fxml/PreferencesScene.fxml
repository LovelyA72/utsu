<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane fx:id="root" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.utsusynth.utsu.controller.PreferencesController">
    <bottom>
        <ButtonBar prefHeight="40.0" BorderPane.alignment="CENTER">
            <buttons>
                <Button fx:id="cancelButton" alignment="CENTER" mnemonicParsing="false" onAction="#closePreferences" text="Cancel" />
                <Button fx:id="applyButton" alignment="CENTER" contentDisplay="CENTER" defaultButton="true" mnemonicParsing="false" onAction="#applyPreferences" text="Apply" />
            </buttons>
            <BorderPane.margin>
                <Insets left="5.0" right="5.0" />
            </BorderPane.margin>
        </ButtonBar>
    </bottom>
   <right>
      <AnchorPane fx:id="anchorRight" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </right>
   <left>
      <AnchorPane fx:id="anchorLeft" prefHeight="200.0" prefWidth="150.0" BorderPane.alignment="CENTER" />
   </left>
</BorderPane>
